define inline_script init
	set description "Executes Python code"
	___run__
	import itertools
	from datamatrix import io
	
	def check_col(col, ref):
		if len(col) != len(ref):
			return False
		for v1, v2 in zip(col, ref):
			if v1 != v2:
				return False
		return True
	__end__
	set _prepare ""

